  "zia_dlp_policies": [
    {
      "policy_id": 1,
      "policy_name": "GDPR-Personal-Data-Protection",
      "rank": 1,
      "state": "enabled",
      "type": "DLP Policy",
      "description": "Prevent exfiltration of EU personal data (GDPR compliance)",
      "apply_to_groups": [
        "All Users"
      ],
      "engines": [
        {
          "name": "EU-Personal-Identifiable-Information",
          "patterns": [
            "EU Passport Numbers",
            "National ID Numbers (EU)",
            "Social Security Numbers (EU)",
            "Tax ID Numbers",
            "Driver's License Numbers (EU)"
          ],
          "threshold": 1
        },
        {
          "name": "Payment-Card-Data",
          "patterns": [
            "Credit Card Numbers (VISA, MasterCard, AMEX)",
            "IBAN",
            "SWIFT/BIC Codes"
          ],
          "threshold": 1
        },
        {
          "name": "Health-Data",
          "patterns": [
            "Medical Record Numbers",
            "Health Insurance Numbers",
            "Prescription Data"
          ],
          "threshold": 1
        }
      ],
      "actions": {
        "email": "block",
        "web_upload": "block",
        "cloud_apps": "block",
        "notification": "user_and_admin"
      },
      "logging": "full",
      "comments": "GDPR personal data protection"
    },
    {
      "policy_id": 2,
      "policy_name": "Financial-Data-Protection",
      "rank": 2,
      "state": "enabled",
      "type": "DLP Policy",
      "apply_to_groups": [
        "All Users"
      ],
      "engines": [
        {
          "name": "Financial-Documents",
          "file_types": [
            ".xlsx",
            ".xls",
            ".csv"
          ],
          "keywords": [
            "Budget",
            "Financial",
            "Quarterly Results",
            "Revenue",
            "P&L"
          ],
          "threshold": 3
        },
        {
          "name": "Payment-Information",
          "patterns": [
            "Credit Card",
            "Bank Account",
            "IBAN"
          ],
          "threshold": 2
        }
      ],
      "actions": {
        "email_external": "block",
        "web_upload": "warn",
        "cloud_apps_unsanctioned": "block"
      },
      "comments": "Financial data exfiltration protection"
    },
    {
      "policy_id": 3,
      "policy_name": "Intellectual-Property-Protection",
      "rank": 3,
      "state": "enabled",
      "type": "DLP Policy",
      "apply_to_groups": [
        "Research-Development",
        "Product-Engineering",
        "IT-DevOps"
      ],
      "engines": [
        {
          "name": "Source-Code",
          "file_types": [
            ".java",
            ".py",
            ".js",
            ".cpp",
            ".cs",
            ".go"
          ],
          "action": "block_external_upload"
        },
        {
          "name": "Design-Documents",
          "file_types": [
            ".dwg",
            ".dxf",
            ".stl",
            ".step"
          ],
          "keywords": [
            "Confidential",
            "Proprietary",
            "Patent"
          ],
          "threshold": 1
        },
        {
          "name": "Trade-Secrets",
          "keywords": [
            "Trade Secret",
            "Proprietary Formula",
            "Confidential Research"
          ],
          "threshold": 1
        }
      ],
      "actions": {
        "email_external": "block",
        "web_upload": "block",
        "cloud_apps_unsanctioned": "block",
        "notification": "security_team"
      },
      "comments": "IP and trade secret protection"
    },
    {
      "policy_id": 4,
      "policy_name": "Executive-Communications-Protection",
      "rank": 4,
      "state": "enabled",
